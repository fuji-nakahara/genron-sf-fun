{
  // Visit https://aka.ms/tsconfig.json to read more about this file
  "compilerOptions": {
    "target": "ES2015",
    "jsx": "react-jsx",
    "sourceMap": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": "./app/javascript",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./app/javascript"]
}
